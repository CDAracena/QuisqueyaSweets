<div class="container py-4">
  <div class="row displayRow">
    <% @cakes.each do |cake| %>
    <div class="imgHolder">
      <div class="col-lg-4"><img src="<%= cake.image %>" width="200" height="200" class="menuImgs">
      </div>
      <p class="itemType py-2">
          <%= cake.name %>
      </p>
      <p class="itemPrice">
        <%= cake.price %>
      </p>
    </div>
    <% end %>
  </div>
</div>
